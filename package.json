{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "apple-messages",
  "title": "Apple Messages",
  "description": "Interact with Apple Messages in Raycast",
  "icon": "icon.png",
  "author": "Vanluren",
  "categories": [
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show Messages",
      "description": "Show your Apple Messages",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.52.1",
    "better-sqlite3": "^8.4.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/better-sqlite3": "^7.6.4",
    "@types/lodash": "^4.14.195",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
